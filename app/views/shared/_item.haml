.itemList__element.element{ data: {id: "#{item.id}"}}
  .element__itemImage{style: "background-image: url('#{ item.image_url.url}') "}
  =link_to "#{item.name}",item_path(item.id), class: "element__itemName"
  .element__itemStorage=item.storage
  %hr
  =link_to "削除",item_path(item.id), class: "element__itemDelete", method: :delete
  =link_to "編集",edit_item_path(item.id), class: "element__itemEdit"
  =link_to "",item_path(item.id), class: "element__itemLink"
